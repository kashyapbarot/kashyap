<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="group_tags_admin" model="res.groups">
        <field name="name">Tags admin</field>
        <field name="category_id" ref="base.module_category_sales_sales"/>
        <field name="implied_ids" eval="[(4, ref('group_tags'))]"/>

    </record>
     <record id="group_tags" model="res.groups">
        <field name="name">Tags user</field>
        <field name="category_id" ref="base.module_category_sales_sales"/>
    </record>
    <record id="group_tags_rules" model="ir.rule">
        <field name="name">See only own Record</field>
        <field ref="model_doc_tag_master" name="model_id"/>
        <field name="domain_force">[('user_id', '=', user.id)]</field>
        <field name="groups"
               eval="[(4, ref('test.group_tags'))]"/>
    </record>
      <record id="group_tags_rules_admin" model="ir.rule">
        <field name="name">See all own Record</field>
        <field ref="model_doc_tag_master" name="model_id"/>
        <field name="domain_force">[]</field>
        <field name="groups"
               eval="[(4, ref('test.group_tags_admin'))]"/>
    </record>
</odoo>